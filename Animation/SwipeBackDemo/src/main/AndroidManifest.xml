<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="jp.sinya.swipeback.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".LibraryActivity">
        </activity>
        <activity android:name=".LibraryItem1Activity">
        </activity>
        <activity android:name=".Library2FragmentActivity">
        </activity>
        <activity android:name=".Library2Activity">
        </activity>
        <activity android:name=".Library3Activity">
        </activity>
        <activity
            android:name=".Library4Activity"
            android:theme="@style/AnimationTheme">
        </activity>
        <activity
            android:name=".Library4ItemActivity"
            android:theme="@style/AnimationTheme">
        </activity>
    </application>

</manifest>